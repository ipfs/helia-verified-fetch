{
  "name": "@helia/verified-fetch",
  "version": "3.2.1",
  "description": "A fetch-like API for obtaining verified & trustless IPFS content on the web",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/ipfs/helia-verified-fetch/tree/main/packages/verified-fetch#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/helia-verified-fetch.git"
  },
  "bugs": {
    "url": "https://github.com/ipfs/helia-verified-fetch/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "keywords": [
    "IPFS",
    "fetch",
    "helia"
  ],
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ],
      "src/*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./plugins": {
      "types": "./dist/src/plugins/plugins.d.ts",
      "import": "./dist/src/plugins/plugins.js"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "breaking": true,
              "release": "major"
            },
            {
              "revert": true,
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "deps",
              "release": "patch"
            },
            {
              "scope": "no-release",
              "release": false
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "chore",
                "section": "Trivial Changes"
              },
              {
                "type": "docs",
                "section": "Documentation"
              },
              {
                "type": "deps",
                "section": "Dependencies"
              },
              {
                "type": "test",
                "section": "Tests"
              }
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "doc-check": "aegir doc-check",
    "build": "aegir build",
    "docs": "aegir docs",
    "test": "aegir test",
    "test:chrome": "aegir test -t browser --cov",
    "test:chrome-webworker": "aegir test -t webworker",
    "test:firefox": "aegir test -t browser -- --browser firefox",
    "test:firefox-webworker": "aegir test -t webworker -- --browser firefox",
    "test:node": "aegir test -t node --cov",
    "test:electron-main": "aegir test -t electron-main",
    "release": "aegir release"
  },
  "dependencies": {
    "@helia/block-brokers": "^4.2.1",
    "@helia/car": "^4.1.1",
    "@helia/delegated-routing-v1-http-api-client": "^4.2.5",
    "@helia/interface": "^5.3.1",
    "@helia/ipns": "^8.2.2",
    "@helia/routers": "^3.1.1",
    "@helia/unixfs": "^5.0.2",
    "@ipld/car": "^5.4.2",
    "@ipld/dag-cbor": "^9.2.3",
    "@ipld/dag-json": "^10.2.4",
    "@ipld/dag-pb": "^4.1.5",
    "@libp2p/interface": "^2.10.1",
    "@libp2p/kad-dht": "^15.1.1",
    "@libp2p/logger": "^5.1.17",
    "@libp2p/peer-id": "^5.1.4",
    "@libp2p/webrtc": "^5.2.14",
    "@libp2p/websockets": "^9.2.12",
    "@multiformats/dns": "^1.0.6",
    "cborg": "^4.2.11",
    "file-type": "^20.5.0",
    "helia": "^5.4.1",
    "interface-blockstore": "^5.3.1",
    "interface-datastore": "^8.3.1",
    "ipfs-unixfs-exporter": "^13.6.2",
    "ipns": "^10.0.2",
    "it-map": "^3.1.3",
    "it-pipe": "^3.0.1",
    "it-tar": "^6.0.5",
    "it-to-browser-readablestream": "^2.0.11",
    "libp2p": "^2.8.7",
    "multiformats": "^13.3.6",
    "progress-events": "^1.0.1",
    "quick-lru": "^7.0.1",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "@helia/dag-cbor": "^4.0.5",
    "@helia/dag-json": "^4.0.5",
    "@helia/http": "^2.1.1",
    "@helia/json": "^4.0.5",
    "@libp2p/crypto": "^5.1.3",
    "@types/sinon": "^17.0.4",
    "aegir": "^47.0.21",
    "blockstore-core": "^5.0.2",
    "browser-readablestream-to-it": "^2.0.9",
    "datastore-core": "^10.0.2",
    "helia": "^5.4.1",
    "ipfs-unixfs-importer": "^15.3.2",
    "it-all": "^3.0.8",
    "it-last": "^3.0.8",
    "it-to-buffer": "^4.0.9",
    "magic-bytes.js": "^1.12.1",
    "p-defer": "^4.0.1",
    "sinon": "^20.0.0",
    "sinon-ts": "^2.0.0"
  },
  "browser": {
    "./dist/src/utils/libp2p-defaults.js": "./dist/src/utils/libp2p-defaults.browser.js"
  },
  "sideEffects": false
}
